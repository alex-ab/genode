<config verbose="yes">

	<parent-provides>
		<service name="ROM"/>
		<service name="PD"/>
		<service name="CPU"/>
		<service name="LOG"/>
		<service name="VM"/>
		<service name="Gui"/>
		<service name="Rtc"/>
		<service name="Nic"/>
		<service name="Timer"/>
	</parent-provides>

	<default caps="100"/>

	<start name="ram_block">
		<resource name="RAM" quantum="270M" />
		<provides><service name="Block"/></provides>
		<config file="tc-firefox.raw" block_size="512"/>
		<route>
			<any-service> <parent/> </any-service>
		</route>
	</start>

	<start name="seoul" caps="720">
		<binary name="seoul"/>
		<resource name="RAM" quantum="1024M"/>
		<route>
			<service name="ROM" label="platform_info">
				<parent label="platform_info"/> </service>
			<service name="ROM" label="config">
				<parent label="vm_seoul.cfg"/> </service>
			<service name="Block">      <child name="ram_block"/></service>
			<any-service><parent/></any-service>
		</route>
	</start>
</config>
